import java.util.List;

public class Input {
		String name;
		String value;

		@XmlAttribute(name="name")
		public void setName(String name) {
    			this.name = name;
		}

		public String getName() {
    			return this.name;
		}

		@XmlAttribute(name="value")
		public void setValue(String value) {
    			this.value = value;
		}

		public String getValue() {
    			return this.value;
		}
}
